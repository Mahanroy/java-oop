import java.security.SecureRandom;

public class Craps {
    private static final SecureRandom randomNumbers = new SecureRandom();
    private static final int SNAKE_EYES = 2;
    private static final int TREY = 3;
    private static final int SEVEN = 7;
    private static final int YO_EVEN = 11;
    private static final int BOX_CARS = 12;

    public Craps() {
    }

    public static int rollDie() {
        int var0 = 1 + randomNumbers.nextInt(6);
        int var1 = 1 + randomNumbers.nextInt(6);
        int var2 = var0 + var1;
        System.out.printf("Player rolled %d + %d = %d%n", var0, var1, var2);
        return var2;
    }

    public static void main(String[] var0) {
        int var1 = 0;
        int var3 = rollDie();
        Status var2;
        switch (var3) {
            case 2:
            case 3:
            case 12:
                var2 = Craps.Status.LOST;
                break;
            case 4:
            case 5:
            case 6:
            case 8:
            case 9:
            case 10:
            default:
                var2 = Craps.Status.CONTINUE;
                var1 = var3;
                System.out.printf("My point is  %d%n", var3);
                break;
            case 7:
            case 11:
                var2 = Craps.Status.WON;
        }

        while(var2 == Craps.Status.CONTINUE) {
            var3 = rollDie();
            if (var3 == var1) {
                var2 = Craps.Status.WON;
            } else if (var3 == 7) {
                var2 = Craps.Status.LOST;
            }
        }

        if (var2 == Craps.Status.WON) {
            System.out.println("Player wins");
        } else {
            System.out.println("Player loses");
        }

    }

    private static enum Status {
        CONTINUE,
        WON,
        LOST;

        private Status() {
        }
    }
}
