import java.util.Locale;

public class GradeBook {
    private String courseName;
    private int[][] grades;

    public GradeBook(String var1, int[][] var2) {
        this.courseName = var1;
        this.grades = var2;
    }

    public void setCourseName(String var1) {
        this.courseName = var1;
    }

    public String getCourseName() {
        return this.courseName;
    }

    public void processGrades() {
        this.outputGrades();
        System.out.printf("%n%s %d%n%s %d%n%n", "Lowest grade in the grade book is", this.getMinimum(), "Highest grade in the grade book is", this.getMaximum());
        this.outputBarChart();
    }

    public int getMinimum() {
        int var1 = this.grades[0][0];
        int[][] var2 = this.grades;
        int var3 = var2.length;

        for(int var4 = 0; var4 < var3; ++var4) {
            int[] var5 = var2[var4];
            int[] var6 = var5;
            int var7 = var5.length;

            for(int var8 = 0; var8 < var7; ++var8) {
                int var9 = var6[var8];
                if (var9 < var1) {
                    var1 = var9;
                }
            }
        }

        return var1;
    }

    public int getMaximum() {
        int var1 = this.grades[0][0];
        int[][] var2 = this.grades;
        int var3 = var2.length;

        for(int var4 = 0; var4 < var3; ++var4) {
            int[] var5 = var2[var4];
            int[] var6 = var5;
            int var7 = var5.length;

            for(int var8 = 0; var8 < var7; ++var8) {
                int var9 = var6[var8];
                if (var9 > var1) {
                    var1 = var9;
                }
            }
        }

        return var1;
    }

    public double getAverage(int[] var1) {
        int var2 = 0;
        int[] var3 = var1;
        int var4 = var1.length;

        for(int var5 = 0; var5 < var4; ++var5) {
            int var6 = var3[var5];
            var2 += var6;
        }

        return (double)var2 / (double)var1.length;
    }

    public void outputBarChart() {
        System.out.println("Overall grade distribution:");
        int[] var1 = new int[11];
        int[][] var2 = this.grades;
        int var3 = var2.length;

        for(int var4 = 0; var4 < var3; ++var4) {
            int[] var5 = var2[var4];
            int[] var6 = var5;
            int var7 = var5.length;

            for(int var8 = 0; var8 < var7; ++var8) {
                int var9 = var6[var8];
                ++var1[var9 / 10];
            }
        }

        for(int var10 = 0; var10 < var1.length; ++var10) {
            if (var10 == 10) {
                System.out.printf("%5d: ", 100);
            } else {
                System.out.printf("%02d-%02d: ", var10 * 10, var10 * 10 + 9);
            }

            for(var3 = 0; var3 < var1[var10]; ++var3) {
                System.out.print("*");
            }

            System.out.println();
        }

    }

    public void outputGrades() {
        System.out.printf("The grades are:%n%n");
        System.out.print("            ");

        int var1;
        for(var1 = 0; var1 < this.grades[0].length; ++var1) {
            System.out.printf("Test %d  ", var1 + 1);
        }

        System.out.println("Average");

        for(var1 = 0; var1 < this.grades.length; ++var1) {
            System.out.printf("Student %2d", var1 + 1);
            int[] var2 = this.grades[var1];
            int var3 = var2.length;

            for(int var4 = 0; var4 < var3; ++var4) {
                int var5 = var2[var4];
                System.out.printf("%8d", var5);
            }

            double var6 = this.getAverage(this.grades[var1]);
            System.out.printf(Locale.US, "%9.2f%n", var6);
        }

    }
}
