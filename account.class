
public class Account {
    private String name;
    private double balance;

    public Account(String var1, double var2) {
        this.name = var1;
        if (var2 > 0.0) {
            this.balance = var2;
        }

    }

    public void setName(String var1) {
        this.name = var1;
    }

    public String getName() {
        return this.name;
    }

    public void deposit(double var1) {
        if (var1 > 0.0) {
            this.balance += var1;
        }

    }

    public double getBalance() {
        return this.balance;
    }
}
